<p id="notice"><%= notice %></p>

<div class="panel panel-default">
  <div class="panel-body">
    <p>
      <strong>Date:</strong>
      <%= @post.created_at.to_date %>
    </p>

    <p>
      <strong>Title:</strong>
      <%= @post.title %>
    </p>
 

    <p>
      <strong>Body:</strong>
      <%= @post.body %>
    </p>

    <%= link_to 'Edit', edit_post_path(@post), class: 'btn btn-primary' %> |
    <%= link_to 'Back', posts_path, class: 'btn btn-primary' %>
  </div>
</div>
